g=int(input())
l=list(map(int,input().split()))
c=[]
while l:
    n=[]
    r=[]
    ma=max(l)
    c.append(ma)
    r.append(ma)
    l.remove(ma)

    while l:
        mi=min(l)
        if mi not in r:
            r.append(mi)
            l.remove(mi)
        else:
            n.append(mi)
            l.remove(mi)
    l=n.copy()
print(len(c))
